pkgname=cgvg-git
pkgver=git
pkgrel=1
pkgdesc="Grep with numerical goto"
arch=('any')
license=('MIT')
url="https://github.com/forgottenswitch/cgvg"
groups=('base' 'base-devel')
provides=('cgvg')
conflicts=('cgvg')
depends=('sh' 'awk' 'ag')
makedepends=()
source=(git+https://github.com/forgottenswitch/cgvg.git)
sha256sums=('SKIP')

prepare() {
  true
}

build() {
  true
}

package() {
  cd ${srcdir}/${pkgname}
  make DESTDIR=${pkgdir} install
}
